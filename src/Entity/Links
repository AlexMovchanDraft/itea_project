<?php

namespace App\Controller;


use Doctrine\ORM\Mapping as ORM;

/**
 *
 * @ORM\Links
 * @ORM\Table(name='links')
 */

class Links {

    /**
     * @var ineger
     */
    private $id;

    /**
     * @var string
     */
    private $encoded_link;

    /**
     * @var string
     */
    private $decoded_link;

    /**
     * @var string
     */
    private $link_name;

    /**
     * @param $encoded_link
     * @return string
     */
    public function getEncodedLink() {
        return $this->encoded_link;
    }

    /**
     * @param $encoded_link
     */
    public function setEncodedLink($encoded_link) {
        $this->encoded_link = $encoded_link;
    }

    /**
     * @param $decoded_link
     * @return mixed
     */
    public function getDecodedLink() {
        return $this->decoded_link;
    }

    /**
     * @param $decoded_link
     */
    public function setDecodedLink($decoded_link) {
        $this->decoded_link = $decoded_link;
    }

    /**
     * @return string
     */
    public function getLinkName() {
        return $this->link_name;
    }

    /**
     * @param $link_name
     */
    public function setLinkName($link_name) {
        $this->link_name = $link_name;
    }

    /**
     * @return ineger
     */
    public function getLinkId() {
        return $this->id;
    }
}